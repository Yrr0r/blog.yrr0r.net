<!DOCTYPE html>
<head>
	<meta charset="utf-8">
	<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bulma@0.9.3/css/bulma.min.css">
	<meta name="viewport" content="width=device-width, initial-scale=1">
</head>

<body>
	<section class="section">
		<div class="columns">
			<% if(indextext != '') { %>
				<div class="column">
					<div class="box">
						<div class="content">
							<%- indextext %> 
						</div>
					</div>
				</div>
			<% } %> 
			<div class="column">
				<% for(each in links){ %> 
					<div class="block">
						<a href="<%- each %> "><%- each %></a> 
						<p><%- links[each].attributes %> </p>
					</div>
				<% } %> 
			</div>
		</div>
	</section>
</body>